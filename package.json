{
  "name": "granfino-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "tools/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "bookcase-builder": "^0.0.9",
    "eslint-config-granfino": "workspace:*",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "turbo": "latest",
    "typescript": "^4.5.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@6.32.9"
}
